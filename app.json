{
  "expo": {
    "name": "Rating",
    "slug": "Rating",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconRating.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/RatingSplash.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconRating.png",
        "backgroundColor": "#293B5F"
      },
      "package": "com.cihan.rating"
    },
    "web": {
      "favicon": "./assets/faviconRating.png"
    }
  }
}
